{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true
    },
        "include": [
        "src/**/*.ts"
    ],
        "exclude": [
        "node_modules",
        "dist"
    ]
}